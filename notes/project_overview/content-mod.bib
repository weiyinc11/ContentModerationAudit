@article{noauthor_understanding_nodate,
	title = {Understanding {South} {African} {Mobile} {Users}' {Perceptions} and {Management} of {Privacy} on {Facebook} and {Related} {Applications}},
	volume = {1},
	abstract = {Social media usage is widespread globally, and continues to grow in developing countries. However, researchers have tended to study social media usage in developed countries and focused less on privacy concerns. Yet, to properly tailor social media privacy settings to the needs of users in less well-resourced settings, we need to study users in a wider range of settings. In this study, we show how mobile social media users in South Africa are making use of the privacy settings and controls on social media platforms. We present findings from interviews of 52 current mobile social media users in South Africa, ranging from low-income users to upper-middle class users. Several themes emerged. First, users’ primary privacy-related concern was who else could see their posts and messages, not what data the platforms collect about them. Second, users displayed general knowledge gaps on both existing social media privacy settings and data collection efforts by advertisers. Third, users’ considerations for their own physical safety often shaped their attitudes towards privacy online. Fourth, usage of privacy settings and conceptions of privacy are heavily swayed by offline social factors, such as perceived intimacy of a platform and information sharing amongst friends. Based on these findings, we make recommendations for social media designers, companies, and regulators to better serve user privacy needs in less well-resourced settings.},
	language = {en},
	number = {3},
	pages = {30},
	file = {Understanding South African Mobile Users' Percepti.pdf:/Users/marshini/Zotero/storage/LP3UNCQW/Understanding South African Mobile Users' Percepti.pdf:application/pdf},
}

