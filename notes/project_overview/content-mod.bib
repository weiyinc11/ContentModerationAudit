	keywords = {Computer Science - Human-Computer Interaction},
	pages = {763--776},
	annote = {Comment: ACM DIS Conference on Designing interactive systems, June 28 - July 2, 2021, online. ACM, New York, NY, USA},
	file = {Bongard-Blanchy et al. - 2021 - I am Definitely Manipulated, Even When I am Aware .pdf:/Users/marshini/Zotero/storage/8LCWRW65/Bongard-Blanchy et al. - 2021 - I am Definitely Manipulated, Even When I am Aware .pdf:application/pdf},
